.login{
	width: 100%;
	height: 400rpx;
	display: flex;
	flex-direction: column;
	justify-content: center;
}
.login button{
	width: 300rpx;
	height: 90rpx;
	margin: 0 auto;
	line-height: 90rpx;
	background-color: #ffffff;
	border: 0rpx;
	font-size: 40rpx;
	border-radius: 60rpx;
}
.content{
	width: 90%;
	margin: 30rpx auto;
}
.content .con_top{
	height: 60rpx;
	line-height: 60rpx;
	margin-bottom: 30rpx;
}
.content .con_top text{
	font-size: 36rpx;
	/* 字间距 */
	letter-spacing:.1em;  
	font-weight: 600;
}
.content .con_top navigator{
	float: right;
	letter-spacing:.1em;
	font-size: 30rpx;
	color: #999999;
}
.content .con_c navigator{
	width: 100%;
	height: 120rpx;
	display: flex;
	margin-bottom: 40rpx;
}
.content .con_c .con_l{
	height: 120rpx;
	font-size: 50rpx;
	line-height: 120rpx;
}
.content .con_c .con_r{
	font-size: 30rpx;
	display: flex;
	justify-content: space-between;
	margin-left: 30rpx;
	flex-grow: 1;
	height: 130rpx;
	border-bottom: 1rpx solid #dbdbdb;
}
.content .con_c .con_r .con_r_l{
	line-height: 2;
	width: 180rpx;
}
.content .con_c .con_r .con_r_r{
	display: flex;
}
.content .con_c .con_r .con_r_r view:nth-child(1){
	margin-left: 120rpx;
	width: 33%;
	height: 60rpx;
	margin-top: 48rpx;
	line-height: 1.1;
	color: #999999;
	font-size: 22rpx;
}
.content .con_c .con_r .con_r_r view:nth-child(2){
	margin-top: 50rpx;
	margin-left: 20rpx;
	text-align: center;
	height: 40rpx;
	line-height: 40rpx;
	border-radius: 5rpx;
	font-size: 23rpx;
	width: 120rpx;
	background-color: #dbdbdb;
}
.content .con_c .con_r .con_r_r text{
	margin-top: 50rpx;
	margin-left: 36rpx;
	color: #999999;
}